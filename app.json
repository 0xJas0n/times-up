{
  "expo": {
    "name": "Time's Up!",
    "slug": "times-up",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      "expo-location"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.mad.timesup",
      "infoPlist": {}
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mad.timesup",
      "permissions": [
        "ACCESS_FINE_LOCATION"
      ]
    }
  }
}
